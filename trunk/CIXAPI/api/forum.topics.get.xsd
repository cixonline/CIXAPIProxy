<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="NewDataSet" targetNamespace="http://cixonline.com" xmlns:mstns="http://cixonline.com" xmlns="http://cixonline.com" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="TopicResultSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Count" type="xs:string" minOccurs="0" />
        <xs:element name="Start" type="xs:string" minOccurs="0" />
        <xs:element name="Topics" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Topic" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Desc" type="xs:string" minOccurs="0" />
                    <xs:element name="Files" type="xs:string" minOccurs="0" />
                    <xs:element name="Flag" type="xs:string" minOccurs="0" />
                    <xs:element name="Name" type="xs:string" minOccurs="0" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="TopicResultSet" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>